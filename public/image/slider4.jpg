        jQuery(function($) {
            function updateTimer(target, endTime) {
                var future = Date.parse(endTime);
                var now = new Date();
                var diff = future - now;

              

                var days = Math.floor(diff / (1000 * 60 * 60 * 24));
                var hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
                var mins = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
                var secs = Math.floor((diff % (1000 * 60)) / 1000);

                // Format numbers as two digits with leading zeros if necessary
                days = ('0' + days).slice(-2);
                hours = ('0' + hours).slice(-2);
                mins = ('0' + mins).slice(-2);
                secs = ('0' + secs).slice(-2);

                // Update the timer display with square boxes and corresponding text
                $(target).html(
                    '<div class="square">' +
                        '<div class="number">' + days + '</div>' +
                        '<div class="label">Ngày</div>' +
                    '</div>' +
                    '<div class="square">' +
                        '<div class="number">' + hours + '</div>' +
                        '<div class="label">Giờ</div>' +
                    '</div>' +
                    '<div class="square">' +
                        '<div class="number">' + mins + '</div>' +
                        '<div class="label">Phút</div>' +
